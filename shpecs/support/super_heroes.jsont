#!/usr/bin/env jsont

arg         squadName  "${squadName:-Super hero squad}"
arg_boolean active     "${active:-true}"
arg_int     formed     "${formed:-2016}"
arg         homeTown   "${homeTown:-Metro City}"
arg         secretBase "${secretBase:-Super tower}"
arg_jsonl   members     $(
  for args in "${@:-}"; do
    eval member.jsont $args
  done
)


__JQ__


{
  $squadName,
  $active,
  $formed,
  location: {
    $homeTown,
    $secretBase
  },
  $members
}
